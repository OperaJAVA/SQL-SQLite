<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/feedFragment">
    <fragment
        android:id="@+id/feedFragment"
        android:name="ru.netology.netology1stproject.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed" >
        <action
            android:id="@+id/action_feedFragment_to_newPostFragment2"
            app:destination="@id/newPostFragment" />
        <action
            android:id="@+id/action_feedFragment_to_onePostFragment"
            app:destination="@id/onePostFragment" />
    </fragment>

    <fragment
        android:id="@+id/newPostFragment"
        android:name="ru.netology.netology1stproject.NewPostFragment"
        android:label="NewPostFragment" />


    <fragment
        android:id="@+id/onePostFragment"
        android:name="ru.netology.netology1stproject.utils.OnePostFragment"
        android:label="OnePostFragment" >
        <action
            android:id="@+id/action_onePostFragment_to_newPostFragment2"
            app:destination="@id/newPostFragment" />
    </fragment>
</navigation>